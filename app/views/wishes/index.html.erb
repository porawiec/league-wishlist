<h1> Wish Index </h1>

<h3> Champion Wishes </h3>
<ul>
<% @wishes.each do |wish| %>
    <li><%= link_to "#{wish.champ.name}", champ_path(wish.champ) %> for <%= link_to "#{wish.player.username}", player_path(wish.player) %> </li>
<% end %>
</ul>